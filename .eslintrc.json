{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","airbnb-base","prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "no-tabs": 0,
        "indent": ["error", 4],
        // "space-in-brackets": ["error", "always"],
        "prettier/prettier": ["error"],
        "no-console": 1
    }
}
